{
    "name": "pandtit/health_check",
    "description": "A configurable health check endpoint for Laravel with IP whitelist support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pandtit",
            "email": "pandtit@163.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/config": "^5.8|^6.0|^7.0|^8.0",
        "symfony/http-foundation": "^4.3|^5.0"
    },
    "require-dev": {
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pandtit\\HealthCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pandtit\\HealthCheck\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pandtit\\HealthCheck\\HealthCheckServiceProvider"
            ],
            "aliases": {}
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html=coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}